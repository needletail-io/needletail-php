{
    "name": "needletail/needletail-php",
    "description": "A PHP interface for Needletail.",
    "version": "1.0.7",
    "keywords": [
        "needletail",
        "php"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3.3",
        "ext-json": "*"
    },
    "suggest": {
        "tightenco/collect": "Allows you to call the toCollection method on the Needletail result"
    },
    "autoload": {
        "psr-4": {
            "Needletail\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
